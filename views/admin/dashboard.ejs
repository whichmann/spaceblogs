<h1>Hello user</h1>
POSTS:
<a href="/add-post">Add New</a>

<ol>
    <% data.forEach(post=> { %>
        <li><a href="/post/<%= post._id %>">
                <%= post.title %> &nearr;
            </a>
            <a href="/edit-post/<%= post._id %>">Edit</a>
            <form action="/delete-post/<%= post._id %>?_method=DELETE" method="POST">
                <input type="submit" value="Delete">
            </form>
        </li>
        <% }) %>
</ol>